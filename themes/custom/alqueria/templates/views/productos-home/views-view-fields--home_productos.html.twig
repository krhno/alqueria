  {%
    set classes = [
    'view',
    'view-' ~ id|clean_class,
    'view-id-' ~ id,
    'view-display-id-' ~ display_id,
    dom_id ? 'js-view-dom-id-' ~ dom_id,
    ]
    %}
    <article>
      <div class="img">
        <a href="{{fields.view_taxonomy_term.content}}" data-seo="btn-content">
          {{fields.field_icono_destacado.content}}
        </a>
      </div>
      <div class="info">
        <div class="text">
          <h3 class="titleHeight"><a href="{{fields.view_taxonomy_term.content}}" data-seo="btn-content">{{fields.name.content}}</a></h3>
          <p class="textHeight">{{fields.field_descripcion.content}}</p>
        </div>
      </div>
    </article>
    