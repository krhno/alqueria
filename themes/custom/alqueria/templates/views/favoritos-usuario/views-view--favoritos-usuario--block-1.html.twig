{%
	set classes = [
	dom_id ? 'js-view-dom-id-' ~ dom_id,
	]
	%}	

	<div{{ attributes.addClass(classes) }}>
		<div class="content-title">
			<div class="title">
				<h2>Favoritos</h2>
			</div>
			<div class="filters">
				{{exposed}}
			</div>
		</div>
		<div class="full-list">
			{{rows}}						
			{{empty}}																					
		</div>
		{% if pager %}
			{{ pager }}
		{% endif %}
	</div>