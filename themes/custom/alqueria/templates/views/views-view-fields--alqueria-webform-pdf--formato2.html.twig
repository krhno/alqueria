						{{ fields.webform_submission_value.content }}
					</td>
				</tr>				
				{% if fields.webform_submission_value_14.content is not empty %}
					<tr>
							<td style="color: #505050; font-size: 10px; font-weight: normal; line-height: normal; font-family: Arial, Helvetica, sans-serif; background-color: #FFFFFF;">
								Explicación de Conflictos Señalados
							</td>
							<td style="color: #505050; font-size: 10px; font-weight: normal; line-height: normal; font-family: Arial, Helvetica, sans-serif; background-color: #FFFFFF;">
