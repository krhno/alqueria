{# MODAL pilares - paragraph #}
{% if node.field_bloques_contenido is not empty %}
  <div id="modal-infographic-ods-{{ node.id }}" class="modal-infographic-ods modal fade" role="dialog">
  <div class="modal-dialog modal-center modal-lg">
    <div class="modal-content">
      <i class="demo-icon icon-icon-close1" data-dismiss="modal" aria-label="Close">&#xe81c;</i>
      <div class="modal-body">
        {% for item in node.field_bloques_contenido %}
          <div class="item">
            <a href="#infographic-ods-{{ loop.index }}" class="title icon-icon-arrow" data-toggle="collapse">
              <div class="img">
                <img data-src="{{ file_url(item.entity.field_icono.entity.fileuri) }}" alt="{{ item.entity.field_icono.entity.alt }}" title="{{ item.entity.field_icono.entity.title }}" class="lazyload">
              </div>
              <div class="text">
                <span>{{ item.entity.field_titulo.value }}</span>
              </div>
              <div class="icon">
                <i class="demo-icon icon-icon-arrow-down">&#xe809;</i>
              </div>
            </a>
            <div id="infographic-ods-{{ loop.index }}" class="panel-collapse collapse">
              {{ item.entity.field_descripcion_larga.value|raw }}
            </div>
          </div>
        {% endfor %}
      </div>
    </div>
  </div>
</div>
{% endif %}

