{# Bloque de datos #}
{% if node.field_titulo_seccion_datos is not empty %}
  <section class="slider-axes sustainability back-blue">
    <div class="containerGridp">
      <div class="content">
        <div class="c-header">
          <h2>{{ node.field_titulo_seccion_datos.value }}</h2>
          {% if node.field_descripcion_seccion_datos is not empty %}
            <p>{{ node.field_descripcion_seccion_datos.value|raw }}</p>
          {% endif %}
        </div>
        <div class="c-slider-axes">
          <div class="s-slider-axes owl-dots-gray owl-nav-icons2 owl-carousel owl-theme">
            {% set cols = 3 %}
            {% for row in node.field_datos_alqueria %}
              {% if loop.index0 % cols == 0 %}
                <div class="items">
              {% endif %}
              <div class="item">
                <div class="img">
                  <picture>
                    <img data-src="{{ file_url(row.entity.field_icono.entity.fileuri) }}" alt="{{ row.entity.field_icono.entity.alt }}" title="{{ row.entity.field_icono.entity.alt }}" class="lazyload">
                  </picture>
                </div>
                <div class="info">
                  <h3>{{ row.entity.field_titulo.value }}</h3>
                  <p>{{ row.entity.field_descripcion_corta.value }}</p>
                </div>
              </div>
              {% if (loop.index0 % cols == cols - 1 or loop.last) %}
                </div>
              {% endif %}
            {% endfor %}
          </div>
        </div>
      </div>
    </div>
  </section>
{% endif %}

