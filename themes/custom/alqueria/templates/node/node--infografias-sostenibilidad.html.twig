{# Cabecera otro nodos#}
{% if node.field_descripcion_iconos is not empty %}
  <section class="infographic-banner sustainability">
    <div class="containerr">
      <div class="content">
        <div class="c-header">
          {% if node.field_descripcion_iconos is not empty %}
            <h1>{{ node.title.value }}</h1>
          {% endif %}
          {{ node.field_descripcion_iconos.value|raw }}
        </div>
      </div>
    </div>
  </section>
{% endif %}

{% if node.field_descripcion_iconos is empty %}
  <section class="infographic-header sustainability">
    <div class="containerGridp">
      <div class="content">
        <div class="c-header">
          {% if node.field_descripcion_iconos is empty %}
            <h1>{{ node.title.value }}</h1>
          {% endif %}
        </div>
        {# Cabecera de infografia #}
        {% if node.field_copy_truco_del_chef is not empty or content.field_banner_sostenibilidad is not empty or node.field_url_youtube is not empty %}
          <div class="c-infographic-header">
            {% if node.field_copy_truco_del_chef is not empty or content.field_icono_motivacion is not empty or node.field_descripcion_inferior is not empty %}
              <div class="info-text">
                {% if node.field_copy_truco_del_chef is not empty %}
                  <div class="left">
                    {{ node.field_copy_truco_del_chef.value|raw }}
                  </div>
                {% endif %}
                {% if content.field_icono_motivacion is not empty %}
                  <div class="right">
                    <div class="icon">
                      {{ content.field_icono_motivacion }}
                    </div>
                    <div class="text">
                      <p>{{ node.field_descripcion_inferior.value|raw }}</p>
                    </div>
                  </div>
                {% endif %}
              </div>
            {% endif %}
            {% if content.field_imagen_objetivo is not empty %}
              <div class="info-modal">
                {% if content.field_imagen_objetivo is not empty %}
                  <div class="img">
                    {{ content.field_imagen_objetivo }}
                  </div>
                {% endif %}
                {# Bloque imagenes iniciales pilares - paragraph #}
                {% if node.field_bloques_contenido is not empty %}
                  <div class="s-slider-infographic-ods owl-dots-gray owl-carousel owl-theme">
                    {% for item in node.field_bloques_contenido %}
                      <div class="item">
                        <img data-src="{{ file_url(item.entity.field_icono.entity.fileuri) }}" alt="{{ item.entity.field_icono.entity.alt }}" title="{{ item.entity.field_icono.entity.title }}" class="lazyload">
                      </div>
                    {% endfor %}
                  </div>
                {% endif %}
                {# Boton ver mas - pilares - modal #}
                {% if node.field_bloques_contenido is not empty %}
                  <div class="button" data-toggle="modal" data-target="#modal-infographic-ods-{{ node.id }}">
                    <img data-src="/{{directory}}/assets/img/infographics/header/button.png" alt="Button" title="Button" class="lazyload">
                  </div>
                {% endif %}
              </div>
            {% endif %}
            <div class="info-banner if-img-video">
              <div class="banner {% if node.field_url_youtube is not empty %}video{%else%}img{%endif%}">
                <div class="img-banner">
                  {{ content.field_banner_sostenibilidad }}
                </div>
                {% if node.field_url_youtube is not empty %}
                  {% if 'https://youtu.be/' in node.field_url_youtube.value %}
                    {% set codigo=node.field_url_youtube.value|replace({'https://youtu.be/':''}) %}
                  {% elseif 'https://www.youtube.com/watch?v=' in node.field_url_youtube.value %}
                    {% set codigo=node.field_url_youtube.value|replace({'https://www.youtube.com/watch?v=':''}) %}
                  {% endif %}
                  <div class="video-banner" data-src="https://www.youtube.com/embed/{{codigo}}">
                    <iframe width="100%" height="100%" class="iframe-yt" src="" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
                  </div>
                  <div class="icon-play">
                    <span><i class="demo-icon icon-icon-play">&#xe80d;</i></span>
                  </div>
                {% endif %}
                <div class="layer-black"></div>
              </div>
            </div>
          </div>
        {% endif %}
      </div>
    </div>
  </section>
{% endif %}

