<li class="item-menu">
	<a href="#recipes-mobile" class="a-item-menu icon-icon-arrow" data-toggle="collapse" data-seo="btn-header">
		{{item.title}}<i class="demo-icon icon-icon-arrow-down">&#xe809;</i>
	</a>
	<a href="{{get_config_data('config.settings','recetas')}}" class="a-item-prehome" data-seo="btn-header">
    {{get_config_data('config.settings','Label_prehome')}}<i class="demo-icon icon-icon-redirect">&#xe83a;</i>
	</a>
	<ul id="recipes-mobile" class="no-dics panel-collapse collapse collapse1">
		<li class="item-collapse1">
			<a href="#moments-mobile" class="a-item-collapse1 icon-icon-arrow active" data-toggle="collapse" data-seo="btn-header">
				Momentos<i class="demo-icon icon-icon-arrow-down">&#xe809;</i>
			</a>
			<ul id="moments-mobile" class="no-dics panel-collapse collapse collapse2">
				{% for key, value in obtener_taxonomia('momentos') %}
				{% if loop.index == 1%}
				{% set primerMomento = key %}
				{% endif %}
				<li class="item-collapse2">
					<span class="a-item-collapse2 dataid {% if loop.index == 1%}active{% endif %}" data-id="megamenu-moments" data-tid="{{key}}" data-url="field_enlace_interna_" data-view="recetas_por_momentos_menu" data-mode="mobile" data-seo="btn-header">
						{{value.name}}<i class="demo-icon icon-icon-arrow-right">&#xe80c;</i>
					</span>
				</li>
				{% endfor %}
			</ul>
		</li>
		<li class="item-collapse1">
			<span class="a-item-collapse1 dataid" data-id="megamenu-tips" data-tid="0" data-view="tips_de_cocina_menu" data-mode="mobile" data-toggle="collapse" data-seo="btn-header" data-config="tips_cocina">
				Tips de cocina<i class="demo-icon icon-icon-arrow-right">&#xe80c;</i>
			</span>
		</li>
	</ul>
</li>
